{
  "name": "wp_setup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "browser-sync start --proxy 'CHANGE.local.com' --files 'css/*.css' './*.php'",
    "watch:sass": " onchange 'scss/*.scss' './*.php' -- npm run sass ",
    "sass": "node-sass --include-path scss scss/main.scss  css/main.css --output-style compact && npm run autoprefix",
    "autoprefix": "postcss --use autoprefixer -b 'last 5 versions' css/main.css -d css --map",
    "dev": "parallelshell 'npm run server' 'npm run watch:sass'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.1",
    "babel-preset-es2015": "^6.22.0",
    "bootstrap": "^4.0.0-beta.2",
    "cheerio": "^0.22.0",
    "concat": "0.0.1-security",
    "concat-cli": "^4.0.0",
    "concat-glob-cli": "^0.1.0",
    "install": "^0.10.2",
    "node-minify": "^2.0.3",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "npm": "^5.6.0",
    "onchange": "^3.2.1",
    "postcss": "^5.2.12",
    "postcss-cli": "^2.6.0",
    "uglify-js": "^2.7.5",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "browser-sync": "^2.24.5",
    "parallelshell": "^3.0.2"
  }
}
